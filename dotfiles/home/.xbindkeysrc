# global

"$BROWSER"
  Mod4+w

"$FILEMANAGER"
  Mod4+e

"check=$(echo -e "yes\nno" | rofi -dmenu -p "shutdown your computer?" -theme ~/.themes/rofi/dark.rasi); [ $check == "yes" ] && shutdown now"
  Mod4+x

"$TERMINAL bashtop"
  Mod4+n

"rofi -show run -theme ~/.themes/rofi/dark.rasi"
  Mod4+p

"rofi -show window -theme ~/.themes/rofi/dark.rasi"
  Mod4+Shift+p

"[[ $(top -b -n1 | grep jgmenu) ]] && pkill jgmenu; jgmenu || jgmenu"
  Mod4+g

"amixer -D pulse sset Master 10%+ ; pkill -RTMIN+2 dwmblocks"
  Mod4+Up

"amixer -D pulse sset Master 10%- ; pkill -RTMIN+2 dwmblocks"
  Mod4+Down

"flameshot full -p ~/Screenshots"
  Alt+Shift+s

"flameshot gui -p ~/Screenshots"
  Alt+Shift+p

"~/scripts/./change_layout.sh"
  Mod4+space

# openbox

#"openbox --exit"
#  Mod4+Shift+q

"polybar-msg cmd toggle"
  Mod4+comma

"$TERMINAL"
  Mod4+Shift+Return


